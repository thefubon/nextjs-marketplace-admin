// Сгенерируйте Prisma Client
// npx prisma generate

// Создайте миграции (если есть изменения в схеме)
// npx prisma migrate dev --name init

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}
